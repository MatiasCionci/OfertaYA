@page "/coodenadasparamapa"
@inject IJSRuntime Js
@inject NavigationManager NavigationManager
@inject Blazored.LocalStorage.ILocalStorageService LocalStorage
@rendermode InteractiveServer
<h3>CargarMapaPorCoordenadas</h3>



<input class="form-control col-sm-4" type="text" @bind-value="ubicacionx" />
<input class="form-control col-sm-4" type="text" @bind-value="ubicaciony" />


<h1>@ubicacionx</h1>

<h2>@producto.ProductoName</h2>

<a href="/Mapas/@ubicacionx/@ubicaciony">enviar</a>




@code {
    [Parameter]
    public Producto producto { get; set; }
 
    public string? ubicacionx { get; set; } = "";
    public string? ubicaciony { get; set; } = "";
    private bool isLoaded = false;


    protected async Task enviarCoordenadas()
        {
           
              
                NavigationManager.NavigateTo($"/Mapas?ubicacionx={ubicacionx}");

     


        }
}
