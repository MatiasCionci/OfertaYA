@page
@model SuperOferta.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Supermercado[0].SupermercadoName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supermercado[0].EsPublico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supermercado[0].Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supermercado[0].Correo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supermercado[0].Password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Supermercado) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SupermercadoName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EsPublico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
